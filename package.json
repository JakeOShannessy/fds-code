{
    "name": "fds-vscode",
    "displayName": "FDS-VSCode",
    "description": "Syntax highlighting for FDS input.",
    "version": "0.0.1",
    "publisher": "JOShannessy",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "fds",
            "aliases": ["FDS", "fds"],
            "extensions": [".fds"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "fds",
            "scopeName": "source.fds",
            "path": "./syntaxes/fds.tmLanguage.json"
        }]
    }
}